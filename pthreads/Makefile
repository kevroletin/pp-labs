CFLAGS=-pthread

top="$(CURDIR)"/
ede_FILES=Project.ede Makefile

task1a_out_SOURCES=task1a.cpp
task1a.out_OBJ= task1a.o
CXX= g++
CXX_COMPILE=$(CXX) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
CXX_DEPENDENCIES=-Wp,-MD,.deps/$(*F).P
CXX_LINK=$(CXX) $(CFLAGS) $(LDFLAGS) -L.
task1b_out_SOURCES=task1b.cpp
task1b.out_OBJ= task1b.o
VERSION=1.0
DISTDIR=$(top)-$(VERSION)
top_builddir = 

DEP_FILES=.deps/task1a.P .deps/Threads.P .deps/task1b.P .deps/Threads.P

all: task1a.out task1b.out pp_tests

DEPS_MAGIC := $(shell mkdir .deps > /dev/null 2>&1 || :)
-include $(DEP_FILES)

%.o: %.cpp
	@echo '$(CXX_COMPILE) -c $<'; \
	$(CXX_COMPILE) $(CXX_DEPENDENCIES) -o $@ -c $<

task1a.out: $(task1a.out_OBJ)
	$(CXX_LINK) -o $@ $^ $(LDDEPS)

task1b.out: $(task1b.out_OBJ)
	$(CXX_LINK) -o $@ $^ $(LDDEPS)

.PHONY:pp_tests
pp_tests:
	$(MAKE) -C unit_tests

tags: 
	$(MAKE) -C unit_tests/ $(MFLAGS) $@


clean:
	rm -f *.mod *.o *.obj .deps/*.P .lo

.PHONY: dist

dist:
	rm -rf $(DISTDIR)
	mkdir $(DISTDIR)
	cp $(task1a_out_SOURCES) $(task1b_out_SOURCES) $(ede_FILES) $(DISTDIR)
	$(MAKE) -C unit_tests $(MFLAGS) DISTDIR=$(DISTDIR)/unit_tests dist
	tar -cvzf $(DISTDIR).tar.gz $(DISTDIR)
	rm -rf $(DISTDIR)

Makefile: Project.ede
