CFLAGS=$(USERFLAGS) -Wall

top="$(CURDIR)"/
ede_FILES=Project.ede Makefile

main_out_SOURCES=main.cpp log.cpp
main.out_OBJ= main.o log.o
CXX= g++
CXX_COMPILE=$(CXX) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
CXX_DEPENDENCIES=-Wp,-MD,.deps/$(*F).P
CXX_LINK=$(CXX) $(CFLAGS) $(LDFLAGS) -L.
VERSION=1.0
DISTDIR=$(top)open_mp-$(VERSION)
top_builddir = 

DEP_FILES=.deps/main.P .deps/log.P .deps/log.P

all: main.out test1

DEPS_MAGIC := $(shell mkdir .deps > /dev/null 2>&1 || :)
-include $(DEP_FILES)

%.o: %.cpp
	@echo '$(CXX_COMPILE) -c $<'; \
	$(CXX_COMPILE) $(CXX_DEPENDENCIES) -o $@ -c $<

main.out: $(main.out_OBJ)
	$(CXX_LINK) -o $@ $^ $(LDDEPS)

.PHONY:test1
test1:
	$(MAKE) -C unit_tests

tags: 
	$(MAKE) -C unit_tests/ $(MFLAGS) $@


clean:
	rm -f *.mod *.o *.obj .deps/*.P .lo

.PHONY: dist

dist:
	rm -rf $(DISTDIR)
	mkdir $(DISTDIR)
	cp $(main_out_SOURCES) $(ede_FILES) $(DISTDIR)
	$(MAKE) -C unit_tests $(MFLAGS) DISTDIR=$(DISTDIR)/unit_tests dist
	tar -cvzf $(DISTDIR).tar.gz $(DISTDIR)
	rm -rf $(DISTDIR)

Makefile: Project.ede
	@echo Makefile is out of date!  It needs to be regenerated by EDE.
	@echo If you have not modified Project.ede, you can use 'touch' to update the Makefile time stamp.
